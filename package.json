{
  "name": "@dlghq/eslint-config-dialog",
  "version": "3.4.0",
  "description": "Dialog ESLint config",
  "main": "index.js",
  "files": [
    "src",
    "base.js",
    "index.js"
  ],
  "keywords": [
    "dialog",
    "eslint",
    "config",
    "styleguide"
  ],
  "scripts": {
    "test": "eslint .",
    "preversion": "npm test",
    "postversion": "npm publish",
    "postpublish": "git push --follow-tags",
    "fmt": "prettier --write \"**/*.{js,md}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Nikita Gusakov <gusnkt@gmail.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=4.2.0"
  },
  "dependencies": {
    "babel-eslint": "^8.0.1"
  },
  "devDependencies": {
    "eslint": "^4.19.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  },
  "peerDependencies": {
    "eslint": "^4.19.0"
  },
  "optionalDependencies": {
    "eslint-plugin-react": "^7.7.0"
  }
}
